{
  "name": "enzyme-on-rtl",
  "version": "0.0.1",
  "type": "module",
  "description": "Automated migration from Enzyme to React Testing Library",
  "bin": {
    "enzyme-on-rtl": "./bin/cli.js"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "cli": "node bin/cli.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.8.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "enzyme",
    "react-testing-library",
    "rtl",
    "migration",
    "testing",
    "converter"
  ],
  "author": "Peter W.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterPCW/enzyme-on-rtl"
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
